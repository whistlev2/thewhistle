{
    "name": "project-boiler",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "npm run build:client && npm run build:server",
        "lint": "eslint --fix **/*.{js,ts,vue}",
        "build:client": "vue-cli-service build",
        "build:server": "babel server --out-dir server-prod --copy-files --extensions \".ts\"",
        "client": "vue-cli-service serve",
        "server": "nodemon server/bin/www --exec babel-node --extensions \".js\",\".ts\" --presets \"@babel/preset-env\",\"@babel/preset-typescript\"",
        "start": "node server-prod/bin/www",
        "test:e2e": "vue-cli-service test:e2e",
        "test:unit": "vue-cli-service test:unit",
        "test": "npm run test:unit && npm run test:e2e",
        "prettier": "pretty-quick"
    },
    "dependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.5",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-typescript": "^7.3.3",
        "axios": "^0.18.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-loader": "^8.0.0-beta.6",
        "babel-preset-env": "^1.7.0",
        "cookie-parser": "~1.4.3",
        "core-js": "^2.6.5",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^8.0.0",
        "express": "~4.16.0",
        "express-subdomain": "^1.0.5",
        "morgan": "~1.9.0",
        "vue": "^2.6.10",
        "vue-class-component": "^7.0.2",
        "vue-property-decorator": "^8.1.0",
        "vue-router": "^3.0.3",
        "vuetify": "^1.5.5"
    },
    "devDependencies": {
        "@babel/node": "^7.0.0",
        "@types/chai": "^4.1.0",
        "@types/cookie-parser": "^1.4.1",
        "@types/cors": "^2.8.5",
        "@types/express": "^4.16.1",
        "@types/mocha": "^5.2.4",
        "@types/morgan": "^1.7.35",
        "@types/node": "^12.0.3",
        "@typescript-eslint/eslint-plugin": "^1.9.0",
        "@typescript-eslint/parser": "^1.9.0",
        "@vue/cli-plugin-e2e-cypress": "^3.8.0",
        "@vue/cli-plugin-eslint": "^3.8.0",
        "@vue/cli-plugin-typescript": "^3.8.0",
        "@vue/cli-plugin-unit-mocha": "^3.8.0",
        "@vue/cli-service": "^3.8.0",
        "@vue/eslint-config-prettier": "^4.0.1",
        "@vue/eslint-config-typescript": "^4.0.0",
        "@vue/test-utils": "1.0.0-beta.29",
        "chai": "^4.1.2",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.13.0",
        "eslint-plugin-react-hooks": "^1.6.0",
        "eslint-plugin-vue": "^5.0.0",
        "prettier": "^1.17.1",
        "pretty-quick": "^1.11.0",
        "sass": "^1.18.0",
        "sass-loader": "^7.1.0",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.1",
        "ts-loader": "^6.0.1",
        "typescript": "^3.4.3",
        "vue-cli-plugin-vuetify": "^0.5.0",
        "vue-eslint-parser": "^6.0.4",
        "vue-template-compiler": "^2.6.10",
        "vuetify-loader": "^1.0.5"
    },
    "engines": {
        "node": "10.x"
    }
}
